{
    "name": "mal-language",
    "displayName": "MAL Language",
    "description": "Syntax highlighting for everyone's favorite interpreted learning-language",
    "version": "0.0.4",
    "publisher": "malworthy",
    "author": "malworthy",
    "icon": "images/CILogo.png",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "mal",
                "aliases": [
                    "mal",
                    "MAL"
                ],
                "extensions": [
                    ".mal"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "mal",
                "scopeName": "source.lox",
                "path": "./syntaxes/lox.tmLanguage.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/danman113/lox-language.git"
    },
    "keywords": [
        "lox",
        "lox-language",
        "vscode",
        "vscode-extension",
        "vscode-language"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/danman113/lox-language/issues"
    },
    "homepage": "https://github.com/danman113/lox-language#readme"
}
